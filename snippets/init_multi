init() {
    get_options "$@"
    get_arguments "$@"

<% if (commands.length>0) {-%>
<% for(var i=0; i<commands.length; i++) {-%>
    add_command <%= commands[i] %>
<% } -%>
<% } -%>

if [[ ! $(check_command ${command}) ]]; then
    usage
    echo
    echo "The command: \"${command}\" is invalid, here the list of valid commands:"
    echo
    list_commands
else
    shift
    . ${command}
fi
}
