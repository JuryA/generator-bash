get_arguments()
{
    ARGS=(
<% for(var i=0; i<args.length; i++) {-%>
  <% if (args[i].varName.length) {-%>
    '<%= args[i].varName%>'
<% } -%>
<% } -%>
    )

    shift $((ARGSHIFT-1))
    COUNT=1
    while [[ $1 != "" && $COUNT -le ${#ARGS[@]} ]];
    do
      eval "${ARGS[$((COUNT-1))]}=$1"
      ((COUNT++))
      shift
    done
}
